local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Metascripter", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab1 = Window:MakeTab({
	Name = "Welcome",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab1:AddParagraph("Welcome","Press any tab to get started.")

local Tab2= Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3= Window:MakeTab({
	Name = "Special",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab4= Window:MakeTab({
	Name = "Custom",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Section = Tab2:AddSection({
	Name = "Scripts"
})

Tab2:AddButton({
	Name = "Infinite Yeild",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
        OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Infinite Yeild Is Now Active, enjoy your commands!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
  	end    
})


Tab3:AddButton({
	Name = "Reboot Metascripter",
	Callback = function()  
	loadstring(game:HttpGet('https://raw.githubusercontent.com/MetaMysteries8/Metascripter-Roblox/main/source'))()
OrionLib:Destroy()
  	end    
})

Tab3:AddButton({
	Name = "Self Destruct",
	Callback = function()  
	OrionLib:Destroy()
  	end    
})


Tab2:AddButton({
	Name = "Doors - Vynixius",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua"))()
   OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Enjoy this doors script!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
  	end    
})

Tab4:AddParagraph("How to use","First delete the default text, then paste in your script, then press enter. Heads up, the text might glitch if the script is long, but the script should still work normally")



Tab4:AddTextbox({
	Name = "Executor",
	Default = "Paste Script Here",
	TextDisappear = true,
	Callback = function(Script)
		loadstring(Script)()
OrionLib:MakeNotification({
	Name = "Metascripter",
	Content = "Script Executed",
	Image = "rbxassetid://4483345998",
	Time = 5
})
	end	  
})




   OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Metascripter Is Now Active",
            Image = "rbxassetid://4483345998",
            Time = 5
        })

   OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Congratulations, you are a beta tester",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
