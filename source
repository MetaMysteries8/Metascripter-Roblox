local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Metascripter", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab1 = Window:MakeTab({
	Name = "Welcome",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab1:AddParagraph("Welcome","Press any tab to get started.")

local Tab2= Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3= Window:MakeTab({
	Name = "Special",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab4= Window:MakeTab({
	Name = "Custom",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Section = Tab2:AddSection({
	Name = "Scripts"
})

Tab2:AddButton({
	Name = "Infinite Yeild",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
        OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Infinite Yeild Is Now Active, enjoy your commands!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
  	end    
})


Tab3:AddButton({
	Name = "Reboot Metascripter",
	Callback = function()  
	loadstring(game:HttpGet('https://raw.githubusercontent.com/MetaMysteries8/Metascripter-Roblox/main/source'))()
OrionLib:Destroy()
  	end    
})

Tab3:AddButton({
	Name = "Self Destruct",
	Callback = function()  
	OrionLib:Destroy()
  	end    
})

Tab2:AddButton({
	Name = "Doors - Vynixius",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua"))()
   OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Enjoy this doors script!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
  	end    
})

local Section = Tab4:AddSection({
	Name = "Custom Script Executor"
})

Tab4:AddParagraph("Hey","Is there a script you want to use, but it's not here? Paste the script you want to execute, then press enter and your script will be ran.")

Tab:AddTextbox({
	Name = "Executor",
	Default = "Paste Script Here",
	TextDisappear = false,
	Callback = function(Script)
		loadstring(Script)
OrionLib:MakeNotification({
	Name = "Title!",
	Content = "Script Executed",
	Image = "rbxassetid://4483345998",
	Time = 5
})
	end	  
})




   OrionLib:MakeNotification({
            Name = "Metascripter",
            Content = "Metascripter Is Now Active",
            Image = "rbxassetid://4483345998",
            Time = 5
        })

